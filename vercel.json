{
	"version": 2,
	"builds": [
	  {
		"src": "build/web/**",
		"use": "@vercel/static"
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "build/web/$1"
	  }
	],
	"headers": [
	  {
		"source": "/(.*)",
		"headers": [
		  {
			"key": "Access-Control-Allow-Origin",
			"value": "*"
		  }
		]
	  }
	]
  }